{
  "name": "mydashboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=22.12.0",
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bunx convex dev --once && concurrently -r bun:dev:web bun:dev:convex",
    "dev:prod": "bash -c 'source ~/.convex-deployments-mydashboard 2>/dev/null || true; if [ \"$CONVEX_URL_PROD\" = \"YOUR_PROD_URL_HERE\" ] || [ -z \"$CONVEX_URL_PROD\" ]; then echo \"‚ùå Production deployment not configured. Run: npm run setup:deployments\"; exit 1; fi && bunx convex dev --once --url $CONVEX_URL_PROD && concurrently -r bun:dev:web \"bunx convex dev --url $CONVEX_URL_PROD\"'",
    "dev:web": "vite dev",
    "dev:ts": "tsc -b -w",
    "dev:convex": "bunx convex dev",
    "build": "vite build",
    "start": "bun run .output/server/index.mjs",
    "start:node": "node .output/server/index.mjs",
    "format": "prettier --write .",
    "setup:deployments": "bash scripts/setup-deployments.sh",
    "verify:deployments": "bash scripts/verify-setup.sh",
    "switch:dev": "bash scripts/switch-deployment.sh dev",
    "switch:prod": "bash scripts/switch-deployment.sh prod",
    "deployments": "npx convex deployments",
    "deploy": "railway up",
    "deploy:railway": "railway up",
    "railway:login": "railway login",
    "railway:logs": "railway logs",
    "ship": "cd /Users/joshuaoliver/Projects && git push && cd mydashboard && railway up"
  },
